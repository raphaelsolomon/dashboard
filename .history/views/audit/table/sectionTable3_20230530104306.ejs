<%- include('./include/header.ejs') %>
	<div class="main-container">
        <div class="min-height-200px">

            <div class="pd-20">
                <h4 class="text-blue h4">-------------</h4>
            </div>
           
            <!-- Simple Datatable start -->
            <div class="card-box mb-30">
                <div class="pd-20">
                    <span>Part 1: Housekeeping</span>
                </div>
                <div class="pb-20">
                    <table class="table hover multiple-select-row data-table-export wrap"> 
                        <thead>
                            <tr>
                                <th class="table-plus datatable-nosort">S/N</th>
                                <th>How is the general house keeping of the bank? </th>
                                <th>Are items stored securely in the right places? </th>
                                <th>Any signs of trailing cable	</th>
                                <th>Other comment  </th>
                                <th class="datatable-nosort">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let i = 0; i <= housekeeping.length - 1; i++) { %>
                            <tr>
                                <td class="table-plus"><%= 1 + i %></td>
                                <td><%= housekeeping[i].general_house_keeping_of_the_bank !== "Good" ? housekeeping[i].general_house_keeping_of_the_bank_where : housekeeping[i].general_house_keeping_of_the_bank %></td>
                                <td><%= housekeeping[i].items_stored_securely_in_right_place !== "Yes" ? housekeeping[i].items_stored_securely_in_right_place_where : housekeeping[i].items_stored_securely_in_right_place %></td>
                                <td><%= housekeeping[i].signs_of_trailing_cable === "Yes" ? housekeeping[i].signs_of_trailing_cable_where : housekeeping[i].signs_of_trailing_cable %></td>
                                <td><%= housekeeping[i].other_comment %></td>
                                <td>
                                    <div class="dropdown">
                                        <a class="btn btn-link font-24 p-0 line-height-1 no-arrow dropdown-toggle"
                                            href="#" role="button" data-toggle="dropdown">
                                            <i class="dw dw-more"></i>
                                        </a>
                                        <div class="dropdown-menu dropdown-menu-right dropdown-menu-icon-list">
                                            <!-- <a class="dropdown-item" href="#"><i class="dw dw-eye"></i> View</a>
                                            <a class="dropdown-item" href="#"><i class="dw dw-edit2"></i> Edit</a> -->
                                            <a class="dropdown-item" href="#"><i class="dw dw-delete-3"></i>
                                                Delete</a>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                          <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
            <!-- Simple Datatable End -->

            <!-- Simple Datatable start -->

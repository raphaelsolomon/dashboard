                        <select class="custom-select2 form-control" name="reporting_injuries_regulatory_body" style="width: 100%; height: 38px">
                                <option value="">Select</option>
                                <option value="Yes">Yes</option>
                                <option value="No">No</option>
                        </select>
                    </div>

					<div class="form-group">
						<label>Date of last reported incident? </label>
						<input class="form-control" name="Date_reported_incident" type="date" placeholder="" />
					</div>
					
				
				</form>
                
			</div>
			<!-- horizontal Basic Forms End -->


                                <tr>
                                    <th>Plastics Retrieved From</th>
                                    <th>Date</th>
                                    <th>Plastic Size</th>
                                    <th>Tonnage</th>
                                    <th>Product</th>
                                    <th>Volume of plastics (pcs)</th>
                                    <th>Initial Content</th>
                                    <th>Manufacturers</th>
                                    <th>ACTION</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% data.tableLimit.forEach(function(e) { %>
                                    <tr class="tr-shadow">
                                        <td>
                                            <%= e.retrieved_from %>

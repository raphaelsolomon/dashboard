<%- include('../includes/header.ejs') %>
    <main>
        <div class="container-fluid px-4">
            <h1 class="mt-4">FORT HERMON - DASHBBORD</h1>
            <ol class="breadcrumb mb-4">
                <li class="breadcrumb-item active">Daily HSE Report Form
                </li>
            </ol>


            <div class="card mb-4">
                <div class="card-header">
                    <i class="fas fa-table me-1"></i>WATER AND OIL RECORD (RECEIVED)
                </div>
                <div class="card-body">
                    <table id="datatablesSimple">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Date</th>
                                <th>Description of deficiencies</th>
                                <th>Recommended Action</th>
                                <th>Action by</th>
                                <th>Target date for close out</th>
                                <th>Remarks</th>
                            </tr>
                        </thead>

                        <tbody>
                            <% inspection.forEach((e)=> { %>
                                <tr>
                                    <td><%= e.id %></td>
                                    <td><%= e.date.to %></td>
                                    <td><%= e.descriptions %></td>
                                    <td><%= e.action %></td>
                                    <td><%= e.action_by %></td>
                                    <td><%= e.close_out_date %></td>
                                    <td><%= e.remarks %></td>
                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </main>
    <%- include('../includes/footer.ejs') %>
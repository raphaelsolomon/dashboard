<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css_/bootstrap.min.css" rel="stylesheet">
    <link href="/css/theme.css" rel="stylesheet">
    <title>Fort Hermon - Data Tracking</title>
</head>

<body class="bg-light">

    <div class="container">
        <div class="row">
            <div class="table-responsive">

                <table class="table table-hover mt-5 table-bordered">
                    <thead class="thead-dark text-center">
                        <tr>
                            <th scope="col" style="font-size: 11px;">#</th>
                            <th scope="col" style="font-size: 11px;"> class="col-sm-2">Date</th>
                            <th scope="col" style="font-size: 11px;">Gate Pass</th>
                            <th scope="col" style="font-size: 11px;">Truck Existing</th>
                            <th scope="col" style="font-size: 11px;">Supplier</th>
                            <th scope="col" style="font-size: 11px;">Truck Plate Number</th>
                            <th scope="col" style="font-size: 11px;">Description</th>
                            <th scope="col" style="font-size: 11px;">Time In</th>
                            <th scope="col" style="font-size: 11px;">Time Out</th>
                            <th scope="col" style="font-size: 11px;">Prepared by</th>
                            <th scope="col" style="font-size: 11px;">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% fort.forEach((e)=> { %>
                            <tr>
                                <th scope="row"><%= e.id %></th>
                                <td style="font-size: 11px;"><%= e.date %></td>
                                <td style="font-size: 11px;"><%= e.reference %></td>
                                <td style="font-size: 11px;"><%= e.existing %></td>
                                <td style="font-size: 11px;"><%= e.supplier %></td>
                                <td style="font-size: 11px;"><%= e.plateno %></td>
                                <td style="font-size: 11px;"><%= e.description %></td>
                                <td style="font-size: 11px;"><%= e.timeIn %></td>
                                <td style="font-size: 11px;"><%= e.timeOut %></td>
                                <td style="font-size: 11px;"><%= e.preparedBy %></td>
                                <td>
                                    <div class="table-data-feature ">
                                        <a href='/admin/hse/input/general/<%= e.id %>'><button class="item " style="margin-right: 10px;"
                                            data-toggle="tooltip " data-placement="top "
                                            title="Edit ">
                                            <i class="zmdi zmdi-edit "></i>
                                        </button></a>
                                    <a href='/admin/hse/table/general/<%= e.id %>'><button class="item "
                                            data-toggle="tooltip " data-placement="top "
                                            title="Delete ">
                                            <i class="zmdi zmdi-delete "></i>
                                        </button></a>
                                    </div>
                                </td>
                            </tr>
                            <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>

</html>
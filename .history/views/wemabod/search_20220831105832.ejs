<%- include('../includes/header.ejs') %>

  <!-- MAIN CONTENT-->
  <div class="main-content">
    <div class="section__content section__content--p30">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <!-- DATA TABLE -->
            <h3 class="title-5 m-b-35">Search Result</h3>
            <div class="table-data__tool">
              <div class="table-data__tool-right">
                <a href="/add"><button class="au-btn au-btn-icon au-btn--green au-btn--small">
                    <i class="zmdi zmdi-plus"></i>add plastic</button></a>
                <div class="rs-select2--dark rs-select2--sm rs-select2--dark2">
                  <select class="js-select2" name="type" onchange="myFunction(this.selectedIndex);">
                    <option selected="selected">Export</option>
                    <option value="csv">CSV</option>
                  </select>
                  <div class="dropDownSelect2"></div>
                </div>
              </div>
            </div>
            <div class="table-responsive table-responsive-data2">
              <table class="table table-data2" id="allFilter">
                <thead>
                  <tr>
                    <th>Date</th>
                    <th>Time-from</th>
                    <th>Time-to</th>
                    <th>Resident car</th>
                    <th>Staff car</th>
                    <th>Visitor car</th>
                    <th>Cab car</th>
                    <th>Delivery</th>
                    <th>All NPF Matters</th>
                    <th>Man centre</th>
                    <th>Hospital</th>
                    <th>Visisting Street Adeojo</th>
                    <th>Visisting Street Eleruwa</th>
                    <th>Visisting Street Olorunimbe</th>
                    <th>Visisting Street Ojora</th>
                    <th>Visisting Street Eric Moore</th>
                    <th>Pedestrian movement Staff</th>
                    <th>Pedestrian movement Visitor</th>
                    <th>Pedestrian movement Resident</th>
                    <th>ACTION</th>
                  </tr>
                </thead>
                <tbody>
                  <% listItem.forEach(function(e) { %>
                    <tr class="tr-shadow">
                      <td>
                        <%= e.date %>
                      </td>
                      <td>
                        <%= e.from_time %>
                      </td>
                      <td>
                        <%= e.to_time %>
                      </td>
                      <td>
                        <%= e.movement_by_vehicle_resident %>
                      </td>
                      <td>
                        <%= e.movement_by_vehicle_staff %>
                      </td>
                      <td>
                        <%= e.movement_by_vehicle_visitor %>
                      </td>
                      <td>
                        <%= e.movement_by_vehicle_cab_service %>
                      </td>
                      <td>
                        <%= e.movement_by_vehicle_delivery %>
                      </td>

                      <td>
                        <%= e.police_matters_all_npf %>
                      </td>
                      <td>
                        <%= e.man_matters %>
                      </td>
                      <td>
                        <%= e.hospital_matters %>
                      </td>
                      <td>
                        <%= e.street_visited_adeojo %>
                      </td>
                      <td>
                        <%= e.street_visited_eleruwa %>
                      </td>
                      <td>
                        <%= e.street_visited_olorunnimbe %>
                      </td>
                      <td>
                        <%= e.street_visited_ojora %>
                      </td>
                      <td>
                        <%= e.street_visited_eric_moore %>
                      </td>
                      <td>
                        <%= e.movement_by_ped_staff %>
                      </td>
                      <td>
                        <%= e.movement_by_ped_visitor %>
                      </td>
                      <td>
                        <%= e.movement_by_ped_resident %>
                      </td>

                      <td>
                        <div class="table-data-feature ">
                          <a href='/details?id=<%= e.id %>'><button class="item "
                            data-toggle="tooltip " data-placement="top "
                            title="Edit ">
                            <i class="zmdi zmdi-edit "></i>
                        </button></a>
                          <a href='/delete/<%= e.id %>'><button class="item " data-toggle="tooltip "
                              data-placement="top " title="Delete ">
                              <i class="zmdi zmdi-delete "></i>
                            </button></a>

                        </div>
                      </td>
                    </tr>
                    <tr class="spacer"></tr>
                    <% }); %>
                </tbody>
              </table>
              <table class="table table-data2" id="retrieved_from">
                <thead id="retrieved_from_hearder">
                  <tr>
                    <th id="headerName">Plastics Retrieved From</th>
                    <th>Date</th>
                    <th>ACTION</th>
                  </tr>
                </thead>
                <tbody id="retrieved_from_body">
                  <tr class="tr-shadow"></tr>
                  <tr class="spacer"></tr>
                </tbody>
              </table>
            </div>
            <!-- END DATA TABLE -->
          </div>
        </div>

        <div class="row">
          <div class="col-md-12">
            <div class="copyright">
              <p>Copyright Â© 2022. All rights reserved. <a href="https://dechconsult.com">Dech Consult Limited</a>.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script>
    function myFunction(selected) {
      window.location = '/recent/2';
    }
  </script>

  <%- include('../includes/footer.ejs') %>